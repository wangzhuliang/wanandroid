/* pages/moudleA//pages/middle/middle.wxss */
.card {
  width: 600rpx;
  height: 800rpx;
  box-shadow: 2px 2px 8px #aaa;
  border-radius: 10rpx;
  background-color: #ffffff;
}

.iv_card {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.v_all {
  position: absolute;
  left: 75rpx;
  bottom: 32rpx;
  right: 75rpx;
}

.v_item {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.v_button {
  height: 100rpx;
  line-height: 100rpx;
  width: 100%;
  color: #EBA356;
  text-align: center;
  background-color: #ffffff;
}