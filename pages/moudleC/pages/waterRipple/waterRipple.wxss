/* pages/moudleC/pages/waterRipple/waterRipple.wxss */
.container{
  width:100%;
  height:800rpx;
  border: 1px solid #EBA356;
}

@keyframes ripple {
  100% {
    transform: scale(2.4);
    background-color: transparent;
  }
}

.ripple {
  background-color: rgba(235,163,86, 0.8);
  border-radius: 100%;
  height:50px;
  width:50px;
  margin-top: -50px;
  position: absolute;
  transform: scale(0);
}
